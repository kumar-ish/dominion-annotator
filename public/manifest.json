{
  "name": "Dominion Annotator",
  "description": "Annotate Dominion cards with desired properties!",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/icon_128.png"
    }
  },
  "permissions": ["storage", "scripting"],
  "content_scripts": [
    {
      "js": ["content.bundle.js"],
      "matches": ["https://dominion.games/*"]
    }
  ],
  "icons": {
    "128": "/images/icon_128.png"
  }
}
